{
  "hosting":[ {
    "public": "build",
    "target": "eats",
    "ignore": [
      "firebase.json",
      "**/.*",  
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  {
    "public": "build",
    "target": "monitor",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }


]
}
